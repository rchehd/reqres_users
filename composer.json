{
    "name": "rchehd/reqres_users",
    "type": "drupal-module",
    "description": "Provides a block listing users from the Reqres dummy API.",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/event-dispatcher": "^6.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpstan": "^1.0",
        "drupal/core": "^10 || ^11",
        "drupal/coder": "^8.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\reqres_users\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\reqres_users\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "drupal/core-composer-scaffold": false,
            "composer/installers": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
